{
    "name": "happyr/annotation-warmer",
    "description": "Warms up your annotation cache",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^3.4 || ^4.3 || ^5.0",
        "symfony/console": "^3.4 || ^4.3 || ^5.0",
        "symfony/serializer": "^3.4 || ^4.3 || ^5.0",
        "symfony/validator": "^3.4 || ^4.3 || ^5.0",
        "symfony/config": "^3.4 || ^4.3 || ^5.0",
        "symfony/debug": "^3.4 || ^4.3 || ^5.0",
        "doctrine/annotations": "^1.8",
        "phpstan/phpstan": "^0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Happyr\\AnnotationWarmer\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Happyr\\AnnotationWarmer\\": "Tests/"
        }
    }
}
